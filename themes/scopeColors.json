{
  "$schema": "vscode://schemas/color-theme",
  "name": "DarkPixel Token Colors",
  "tokenColors": [
    {
      "scope": ["keyword.other.line-continuation.c"],
      "settings": {
        "foreground": "#a0FFa0",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator.heredoc.shell",
        "keyword.control.heredoc-token.shell",
        "keyword.operator.redirect.shell"
      ],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.other.special.shell",
        "variable.other.loop.shell",
        "variable.other.positional.shell"
      ],
      "settings": {
        "foreground": "#FF7700",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.language.makefile",
        "variable.other.bracket.shell",
        "variable.other.normal.shell"
      ],
      "settings": {
        "foreground": "#1DFF00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.scope.function-call.makefile", "string.interpolated.dollar.shell"],
      "settings": {
        "foreground": "#FF7F00cc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function.target.makefile"],
      "settings": {
        "foreground": "#FFFE24",
        "fontStyle": "bold underline"
      }
    },
    {
      "scope": ["meta.scope.prerequisites.makefile"],
      "settings": {
        "foreground": "#68ffd9",
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": ["punctuation.definition.variable.makefile"],
      "settings": {
        "foreground": "#FF0098",
        "fontStyle": "bold italic"
      }
    },

    {
      "scope": ["comment.block.ndocfile.c"],
      "settings": {
        "foreground": "#00CC7777",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.ndocfunc.c"],
      "settings": {
        "foreground": "#00AC0077",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.ndocdef.c"],
      "settings": {
        "foreground": "#7200FFaa",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.ndocconst.c"],
      "settings": {
        "foreground": "#9200FFaa",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.ndocparam.c"],
      "settings": {
        "foreground": "#3498DB99",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.ndocreturn.c"],
      "settings": {
        "foreground": "#00FFF199",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.ndoclink.c"],
      "settings": {
        "foreground": "#DD777755",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["comment.block.ndocbold.c"],
      "settings": {
        "foreground": "#554488cc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["comment.block.ndocunderline.c"],
      "settings": {
        "foreground": "#554488cc",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["comment.block.seperatorline.c"],
      "settings": {
        "foreground": "#4400FFee",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.return.success.c"],
      "settings": {
        "foreground": "#30FF30",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.return.failure.c"],
      "settings": {
        "foreground": "#FF0044",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.return.error.c"],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.value.definition.c"],
      "settings": {
        "foreground": "#FF3000CC",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.size.definition.c"],
      "settings": {
        "foreground": "#FFAF00CC",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.constant.preprocessor.c"],
      "settings": {
        "foreground": "#EEEEEE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.preprocessor.directive.c"],
      "settings": {
        "foreground": "#CCCCFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator.ternary.c"],
      "settings": {
        "foreground": "#FFCF00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.other.preprocessor.c"],
      "settings": {
        "foreground": "#AAFAFF",
        "fontStyle": ""
      }
    },
    {
      "scope": ["string.quoted.other.lt-gt.include.c", "string.quoted.double.include.c"],
      "settings": {
        "foreground": "#BF40FF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["entity.name.function.preprocessor.c", "entity.name.class"],
      "settings": {
        "foreground": "#33FF33BB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type.declaration.c", "entity.name.type.typedef.c"],
      "settings": {
        "foreground": "#FF2020",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator.increment-decrement.c"],
      "settings": {
        "foreground": "#0FFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.block.c", "meta.scope.group.shell", "meta.block.special.c"],
      "settings": {
        "foreground": "#7F77FF",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.function.c"],
      "settings": {
        "foreground": "#33FF33",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.function-call.c"],
      "settings": {
        "foreground": "#8F77FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.parens.c"],
      "settings": {
        "foreground": "#7F88FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.preprocessor.macro.c"],
      "settings": {
        "foreground": "#7C7C7C",
        "fontStyle": "bold"
      }
    },

    {
      "scope": ["string.macro.stringify.c"],
      "settings": {
        "foreground": "#777777",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Components",
      "scope": [
        "entity.name.tag.open.jsx",
        "entity.name.tag.close.jsx",
        "support.class.component.open.jsx",
        "support.class.component.close.jsx"
      ],
      "settings": {
        "foreground": "#FF0050",
        "fontStyle": ""
      }
    },
    {
      "name": "JSX Embedded Punctuation",
      "scope": ["punctuation.definition.tag.jsx"],
      "settings": {
        "foreground": "#FFF000",
        "fontStyle": ""
      }
    },
    {
      "name": "JSX Embedded Punctuation",
      "scope": ["punctuation.section.embedded.begin.jsx", "punctuation.section.embedded.end.jsx"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Nested",
      "scope": ["JSXNested"],
      "settings": {
        "foreground": "#AFFC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Attributes",
      "scope": ["JSXAttrs", "entity.other.attribute-name.jsx"],
      "settings": {
        "foreground": "#AAA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript Module Keywords",
      "scope": ["keyword.control.module.js"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JavaScript Module-String",
      "scope": ["string.quoted.module.js"],
      "settings": {
        "foreground": "#CC5500",
        "fontStyle": "underline"
      }
    },
    {
      "name": "JavaScript 'new'",
      "scope": ["keyword.operator.new.js"],
      "settings": {
        "foreground": "#FF33ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Source->HTML Text",
      "scope": ["text.html.php"],
      "settings": {
        "foreground": "#AFF",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML Double Quoted String",
      "scope": ["string.quoted.double.html"],
      "settings": {
        "foreground": "#667",
        "fontStyle": "italic"
      }
    }
  ]
}
